{
  "name": "rahul1115.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "tailwind.config.js",
  "scripts": {
    "start": "picogen",
    "tailwindcss": "tailwindcss -i ./src/_static/css/input.css -o ./src/_static/css/tailwind.css",
    "tailwindcssWatch": "tailwindcss -w -i ./src/_static/css/input.css -o ./src/_static/css/tailwind.css",
    "generate": "picogen clean & picogen generate",
    "preview": "picogen clean && npm run generate && picogen preview",
    "push": "node -e \"const mssg = 'BUILD - ' + process.argv[1]; require('child_process').execSync('git add . && git commit -m \\\"' + mssg + '\\\" && git push', { stdio:[0, 1, 2] })\"",
    "update": "npm run generate && npm run push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RAHUL1115/rahul1115.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RAHUL1115/rahul1115.github.io/issues"
  },
  "homepage": "https://github.com/RAHUL1115/rahul1115.github.io#readme",
  "dependencies": {
    "picogen": "^4.0.5",
    "tailwindcss": "^3.2.4"
  }
}
